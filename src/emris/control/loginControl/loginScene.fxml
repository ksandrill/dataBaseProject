<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="emris.control.loginControl.LoginSceneController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <VBox alignment="TOP_CENTER" layoutX="195.0" layoutY="99.0" prefHeight="202.0" prefWidth="249.0" spacing="5.0">
               <children>
                  <HBox prefHeight="34.0" prefWidth="149.0">
                     <children>
                        <Label prefHeight="18.0" prefWidth="55.0" text="login" />
                        <TextField fx:id="login" />
                     </children>
                  </HBox>
                  <HBox prefHeight="27.0" prefWidth="250.0">
                     <children>
                        <Label prefHeight="18.0" prefWidth="55.0" text="password" />
                        <PasswordField fx:id="password" alignment="TOP_RIGHT" />
                     </children>
                  </HBox>
                  <HBox prefHeight="34.0" prefWidth="149.0">
                     <children>
                        <Label prefHeight="18.0" prefWidth="55.0" text="host" />
                        <TextField fx:id="host" />
                     </children>
                  </HBox>
                  <HBox prefHeight="34.0" prefWidth="149.0">
                     <children>
                        <Label prefHeight="18.0" prefWidth="55.0" text="port" />
                        <TextField fx:id="port" />
                     </children>
                  </HBox>
                  <HBox prefHeight="96.0" prefWidth="243.0">
                     <children>
                        <Label prefHeight="18.0" prefWidth="55.0" text="sid" />
                        <TextField fx:id="sid" />
                     </children>
                  </HBox>
                  <Button mnemonicParsing="false" onAction="#buttonClicked" text="connect" />
               </children>
            </VBox>
            <Label layoutX="306.0" layoutY="14.0" text="Вход">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
